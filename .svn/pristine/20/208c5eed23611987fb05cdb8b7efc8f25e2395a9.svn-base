package md.autoservice.serviceauto.model;

/**
 *
 * @author iucosoft5
 */
public class Angajat extends Persoana {
    
    String functia;
    

    public Angajat() {
    }

    public Angajat(String functia) {
        this.functia = functia;
    }

    public Angajat(int id, String nume, String prenume, int nrTelefon, String functia) {
        super(id, nume, prenume, nrTelefon);
        this.functia = functia;
    }

    public String getFunctia() {
        return functia;
    }

    public void setFunctia(String functia) {
        this.functia = functia;
    }

    @Override
    public String toString() {
        return "Angajat{" + "functia=" + functia + '}';
    }

}
