    private void jButtonMasinaSaveActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonMasinaSaveActionPerformed

        //proprietar = masina.getProprietar();
        proprietar = new Proprietar();

        proprietar.setNume(jTextFieldProprietarNume.getText());
        proprietar.setPrenume(jTextFieldProprietarPrenume.getText());
        proprietar.setNrTelefon(Integer.valueOf(jTextFieldProprietarTelefon.getText()));
        proprietar.setEmail(jTextFieldEmail.getText());

        masina.setNrInmatriculare(jTextFieldNrInmatriculare.getText());
        masina.setMarca(jTextFieldMarca.getText());
        masina.setModel(jTextFieldModel.getText());
        masina.setAnu((int) jSpinnerAnu.getValue());
        masina.setCombustibil((String) jComboBoxCombustibil.getSelectedItem());
        masina.setModelMotor(jTextFieldModelMotor.getText());
        masina.setProprietar(proprietar);
        try {
            String word = "";
            if (masina.getId() < 1) {//save add new user
                System.out.println("am ajuns la save");
                masinaService.save(masina);
                word = "adaugata";
            } else {// update
                System.out.println("am ajuns la update");
                // Proprietar proprietar1 = new Proprietar();
//                     proprietar = masina.getProprietar();
//                   
//                    proprietarDao.update(proprietar);
                masiniDao.update(masina);
//                    JOptionPane.showMessageDialog(parenFrame, masina);
//
//                                           
                //proprietarDao.update(masina.getProprietar());
                //  masinaService.update(masina);
                word = "editata";
            }
            JOptionPane.showMessageDialog(this, "Masina " + word + " cu success", "Info",
                    JOptionPane.INFORMATION_MESSAGE);

            parenFrame.masiniPanel.refreshMasiniTable();

            masina = null;

           clearClosing();
        } catch (Exception ex) {
            Logger.getLogger(MasinaDialog.class.getName()).log(Level.SEVERE, null, ex);
        }


    }//GEN-LAST:event_jButtonMasinaSaveActionPerformed

    private void jButtonDeleteReparatieActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButtonDeleteReparatieActionPerformed

        int row = jTableReparatii.getSelectedRow();
        int reparatieId = (Integer) jTableReparatii.getValueAt(row, 0);// 0 -0 prima coloana
        try {
            reparatie = reparatiDao.findById(reparatieId);
      

            int rez = JOptionPane.showConfirmDialog(parenFrame, "esti sigur vrei sa sterg reparatia?",
                    "Question", JOptionPane.YES_NO_OPTION, JOptionPane.QUESTION_MESSAGE);

            switch (rez) {
                case JOptionPane.YES_OPTION:
                    //delete all coments of this user
                    //comentariiDao.deleteAllUserCommentsByUserId(userId)
                    
                    reparatieAngajatDao.delete(reparatie.getId());
                    reparatiDao.delete(reparatie);

                    JOptionPane.showMessageDialog(parenFrame, "Reparatie sters cu success", "Succes",
                            JOptionPane.INFORMATION_MESSAGE);
                    break;
                case JOptionPane.NO_OPTION:
                case JOptionPane.CLOSED_OPTION:
                    break;
            }
              } catch (ReparatiNotFoundException ex) {
             JOptionPane.showMessageDialog(parenFrame, "Reparatia nu este gasit cu id=" + reparatieId,
                    "Error", JOptionPane.WARNING_MESSAGE);
            Logger.getLogger(LucratoriPanel.class.getName()).log(Level.SEVERE, null, ex);
        } catch (SQLException ex) {
            Logger.getLogger(LucratoriPanel.class.getName()).log(Level.SEVERE, null, ex);
        }

    
        refreshMasiniTable();
    }//GEN-LAST:event_jButtonDeleteReparatieActionPerformed

