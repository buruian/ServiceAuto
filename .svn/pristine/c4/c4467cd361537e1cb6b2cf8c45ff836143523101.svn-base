    private void fillTable() {

        List<Integer> listaIdAngajatiSelectati = new ArrayList<>();
        List<Angajat> listaAngajatiSelectati = new ArrayList<>();
        try {

            listaIdAngajatiSelectati = reparatieAngajatDao.findAllByIdReparati(reparati.getId());
            for (Integer id : listaIdAngajatiSelectati) {
                angajat = angajatDao.findById(id);
                System.out.println("" + angajat.getFunctia());
                listaAngajatiSelectati.add(angajat);
            }

            angajatiSelectatiTableModel = new AngajatiSelectatiTableModel(listaAngajatiSelectati);
            jTableAngajatiSelectati.setModel(angajatiSelectatiTableModel);
        } catch (AngajatNotFoundException ex) {
            Logger.getLogger(ReparatiiDialog.class.getName()).log(Level.SEVERE, null, ex);
        }
    }

